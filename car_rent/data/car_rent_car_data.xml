<odoo>
    
    <record id="car_max" model="car.rent.driver">
        <field name="name">Max</field>
    </record>

    
    <record id="Bmw" model="car.rent.car">
        <field name="name">BMW</field>
        <field name="model">E46</field>
        <field name="color">black</field>
